        "students = [\"Emma\", \"Adam\", \"Justin\", \"Paul\"]\n",
        "\n",
        "def divide_students(students):\n",
        "    groups = [[], []]\n",
        "    for index, student in enumerate(students):\n",
        "        if index % 2 == 0:\n",
        "            groups[0].append(student)\n",
        "        else:\n",
        "            groups[1].append(student)\n",
        "    print(groups)\n",
        "    return groups\n",
        "\n",
        "st = divide_students(students)\n",
        "st[0]\n",
        "st[1]"
